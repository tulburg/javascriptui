            ["code", "Y29uc3QgZm9vID0gbmV3IENvbnRhaW5lcigpOwpjb25zdCBiYXIgPSBuZXcgSDEoKTsKY29uc3QgZm9vYmFyID0gbmV3IFAoKTsKZm9vLmFkZENoaWxkKGJhciwgZm9vYmFyKTsKCmZvby5jaGlsZHJlbigpOwoKLy8gb3V0cHV0OiBbYmFyLCBmb29iYXJd"]
